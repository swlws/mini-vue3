#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/node_modules:/Users/sunwenlong/Self/Code/node_modules:/Users/sunwenlong/Self/node_modules:/Users/sunwenlong/node_modules:/Users/node_modules:/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/typescript/bin/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/.pnpm/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/node_modules:/Users/sunwenlong/Self/Code/node_modules:/Users/sunwenlong/Self/node_modules:/Users/sunwenlong/node_modules:/Users/node_modules:/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/typescript/bin/node_modules:/Users/sunwenlong/Self/Code/10-MyGit/vue3-core/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
